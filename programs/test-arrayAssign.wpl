extern int func printf(...);

proc printInt5Array(str name, int [5] array) {
  var i := 0; 

  while i < array.length do {

    printf("%s[%d] = %d", name, i, array[i]);

    if(i + 1 < array.length) then {
      printf(", ");
    }

    i := i + 1; 
  }

  printf("\n");
}


proc addOneToInt5(str name, int [5] array) {
  printf("In AddOne\n");
  printInt5Array(name, array);

  {
    var i := 0; 

    while i < array.length do {
      array[i] := array[i] + 1; 
      i := i + 1; 
    }
  }

  printInt5Array(name, array);
  printf("Exiting add one!\n");
}

int func program() {
  int [5] a; 

  {
    var i := 0; 

    while i < a.length do {
      a[i] := i; 
      i := i + 1; 
    }
  }

  printInt5Array("a", a);

  int [5] b; 

  
  {
    var i := 0; 

    while i < b.length do {
      b[i] := 0; 

      i := i + 1; 
    }
  }

  printInt5Array("b", b);

  printf("------------------------------\n");

  printInt5Array("a", a);
  printInt5Array("b", b);

  printf("------------------------------\n");

  b := a; 

  printInt5Array("a", a);
  printInt5Array("b", b);

  printf("------------------------------\n");


  a[3] := 1000; 
  b[0] := 9999;


  printInt5Array("a", a);
  printInt5Array("b", b);

  printf("------------------------------\n");

  addOneToInt5("a", a);
  printInt5Array("a", a);
  
  printf("------------------------------\n");

  return 0; 
}