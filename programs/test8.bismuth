extern func printf(...) -> int;

# int a := 10; 

prog program :: c : -int = {

    int x, y := 0; 
    while x < 4 {
        y := 0; 
        while y < 4 {
            printf("%d, %d\n", x, y);

            y := y + 1; 
        }

        
        x := x + 1; 
        printf("x is %d, x < 4 = %d\n", x, x < 4);
    }

    # return -1; 
    c.send(-1)
}

