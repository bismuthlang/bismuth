extern int func printf(...);

int func program() {

    int func foo(int x) {
        printf("FOO: %u\n", x);
        return x; 
    }

    var ref := foo; 

    ref(10); 
    foo(11); 

    printf("ref (:= foo(1)) = %u\n", ref(1));

    {
        int func foo(int z) {
            printf("Foo2: %u\n", z);
            return z; 
        }

        var ref := foo; 

        ref(10); 
        foo(11); 

        printf("ref (:= foo(1)) = %u\n", ref(1));
    }

    return 0;
}