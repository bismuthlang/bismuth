extern int func printf(...);

# int func mutArgsOne(int i) {
define mutArgsOne :: io : Channel<+int;-int> = {
    int i := io.recv(); 

    if !(5 < 10) {
        i := 20;
    } else {
        i := 50; 
    }
    printf("GOT: %d\n", i);

    # return i; 
    io.send(i)
}


