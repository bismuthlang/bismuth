extern int func printf(...);

str [1] a; 

proc foo() {
    str [1] b := a; 
    printf("foo: a[0]=%s\n", a[0]);
}


int func program () {
    str [1] c := a; 
    foo();

    a[0] := "A";
    printf("a[0] is %s\n", a[0]);
    printf("c[0] is %s\n", c[0]);
    foo();

    a[0] := "B"; 
    printf("a[0] is %s\n", a[0]);
    printf("c[0] is %s\n", c[0]);
    foo();

    
    return -1; 
}