extern int func printf(...);

int func fib(int n, int[5] a) {
  if(n == 0 | n == 1) {
    return n;
  }

  return fib(n - 1, a) + fib(n - 2, a);
}

define program :: c : Channel<-int> = {
 var current := 1;        
 while current < 20 { 
   int[5] b; 
   var temp := b == b; 
   var i := fib(current, b); 
   printf("The %dth fibonacci number is: %d\n", current, i);
   current := current + 1; 
 }
 return -1;
}