# Unused: Global State

extern int func printf(str a,...);

str a := "Hello there";



proc other() {
    printf("other: a=%s\n", a);

    a := "Goodbye!";
    printf("other: a := \"Goodbye!\"\n");

    printf("other: a=%s\n", a);
}

define program :: c : Channel<-int> = {
    printf("a=%s\n", a);

    a := "Hi!";
    printf("a := \"Hi!\"\n");
    printf("a=%s\n", a);

    other(); 

    printf("a=%s\n", a);

    return 0;
}